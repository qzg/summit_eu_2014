[global]
rw=randrw
rwmixread=75
rwmixwrite=25
blocksize=4096
ioengine=sync
norandommap=1
direct=1
iodepth=1
iodepth_batch=1
iodepth_batch_complete=1
group_reporting=1
ramp_time=5
time_based=1
runtime=120s
randrepeat=0
directory=/root/fio
unlink=1
disable_lat=0
disable_clat=0
disable_slat=0
numjobs=1
nrfiles=4
size=1g
filename=scratchfile

[random 75 25 read write latency]
description="random 4096b 75/25 read/write latency, 1 job"




